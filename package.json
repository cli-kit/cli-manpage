{
  "name": "cli-manpage",
  "version": "0.0.9",
  "description": "Man page generation tool",
  "author": "muji <noop@xpm.io>",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/cli-manpage"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/cli-manpage/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/freeformsystems/cli-manpage/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "cli",
    "manual",
    "help",
    "man"
  ],
  "dependencies": {
    "async": "0.2.10",
    "cli-command": "^0.8.222",
    "cli-interface": "^1.0.8",
    "cli-logger": "^0.5.27",
    "ttycolor": "^0.8.12"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "istanbul": "^0.3.2",
    "mocha": "^1.21.4"
  },
  "directories": {
    "bin": "./bin",
    "man": "./doc/man"
  },
  "engine": [
    "node >= 0.10.21"
  ],
  "scripts": {
    "manual": "./bin/manpage --output ./doc/man ./bin/manpage",
    "build": "mdp --force -v",
    "test": "istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/unit/"
  },
  "mdp": {
    "title": {
      "ref": "name",
      "format": "%s(1)"
    },
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/cli-manpage",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md"
        ]
      },
      {
        "title": "Usage",
        "bin": "manpage --help",
        "type": "code"
      },
      {
        "inc": [
          "license.md",
          "footer.md"
        ]
      }
    ]
  }
}
